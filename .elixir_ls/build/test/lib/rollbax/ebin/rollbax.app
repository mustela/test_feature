{application,rollbax,
             [{applications,[kernel,stdlib,elixir,logger,jason,hackney]},
              {description,"Exception tracking and logging from Elixir to Rollbar"},
              {modules,['Elixir.Rollbax','Elixir.Rollbax.Client',
                        'Elixir.Rollbax.Exception','Elixir.Rollbax.Item',
                        'Elixir.Rollbax.Logger','Elixir.Rollbax.Reporter',
                        'Elixir.Rollbax.Reporter.Silencing',
                        'Elixir.Rollbax.Reporter.Standard']},
              {registered,[]},
              {vsn,"0.11.0"},
              {env,[{enabled,true},
                    {custom,#{}},
                    {api_endpoint,<<"https://api.rollbar.com/api/1/item/">>},
                    {enable_crash_reports,false},
                    {reporters,['Elixir.Rollbax.Reporter.Standard']}]},
              {mod,{'Elixir.Rollbax',[]}}]}.
